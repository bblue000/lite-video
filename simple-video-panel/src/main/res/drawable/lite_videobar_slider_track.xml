<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/lite_videobar_slider_track_conner" />
            <solid android:color="@color/lite_videobar_slider_track_bg"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/lite_videobar_slider_track_conner" />
                <solid android:color="@color/lite_videobar_slider_track_secprogress"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/lite_videobar_slider_track_conner" />
                <solid android:color="@color/lite_videobar_slider_track_progress"/>
            </shape>
        </clip>
    </item>


</layer-list>